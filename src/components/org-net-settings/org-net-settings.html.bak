


<ion-grid>
  <ion-row>
    <ion-col col-6>
      <ion-list>
        <ion-item>
          <ion-label>Org</ion-label>
          <ion-select [ngModel]="org?.id" (ngModelChange)="orgFormSelected($event)" class="myCustomSelect">
           <ion-option *ngFor="let o of orgs" [value]="o?.id" [selected]="o?.id == org?.id">{{o.name}}</ion-option>
          </ion-select>
        </ion-item>
      </ion-list>
    </ion-col>
  </ion-row>
  <ion-row>
    <ion-col col-3>
      <ion-label>Net</ion-label>
    </ion-col>
    <ion-col col-9>
      <ion-select [ngModel]="net?.id" (ngModelChange)="netFormSelected($event)" class="myCustomSelect">
       <ion-option *ngFor="let n of nets" [value]="n?.id" [selected]="n?.id == net?.id">{{n.name}}</ion-option>
     </ion-select>
    </ion-col>
  </ion-row>
</ion-grid>
